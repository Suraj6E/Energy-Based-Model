   "source": [
    "import matplotlib.pyplot as plt\n",
    "import cv2\n",
    "\n",
    "# Load the image\n",
    "image = cv2.imread('image.jpg')\n",
    "\n",
    "# Convert BGR to RGB (Matplotlib expects RGB)\n",
    "image_rgb = cv2.cvtColor(image, cv2.COLOR_BGR2RGB)\n",
    "\n",
    "# Plot the image\n",
    "plt.imshow(image_rgb)\n",
    "plt.axis('off')  # Hide axis\n",
    "plt.show()"
   ]
  }
